{
  "version": 1,
  "projects": {
    "hiking": {
      "projectType": "application",
      "root": "apps/hiking",
      "sourceRoot": "apps/hiking/src",
      "prefix": "hk",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/hiking",
            "index": "apps/hiking/src/index.html",
            "main": "apps/hiking/src/main.ts",
            "polyfills": "apps/hiking/src/polyfills.ts",
            "tsConfig": "apps/hiking/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/hiking/src/favicon.ico", "apps/hiking/src/assets"],
            "styles": ["apps/hiking/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/hiking/src/environments/environment.ts",
                  "with": "apps/hiking/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "hiking:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "hiking:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "hiking:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/hiking/src/**/*.ts",
              "apps/hiking/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/hiking/src/test.ts",
            "tsConfig": "apps/hiking/tsconfig.spec.json",
            "karmaConfig": "apps/hiking/karma.conf.js",
            "polyfills": "apps/hiking/src/polyfills.ts",
            "styles": [],
            "scripts": [],
            "assets": []
          }
        }
      }
    },
    "hiking-e2e": {
      "root": "apps/hiking-e2e",
      "sourceRoot": "apps/hiking-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/hiking-e2e/cypress.json",
            "tsConfig": "apps/hiking-e2e/tsconfig.e2e.json",
            "devServerTarget": "hiking:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "hiking:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/hiking-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "hiking-data-access": {
      "projectType": "library",
      "root": "libs/hiking/data-access",
      "sourceRoot": "libs/hiking/data-access/src",
      "prefix": "hkworkspace",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/hiking/data-access/src/**/*.ts",
              "libs/hiking/data-access/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/hiking/data-access"],
          "options": {
            "jestConfig": "libs/hiking/data-access/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "hiking-feature": {
      "projectType": "library",
      "root": "libs/hiking/feature",
      "sourceRoot": "libs/hiking/feature/src",
      "prefix": "hkworkspace",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/hiking/feature/src/**/*.ts",
              "libs/hiking/feature/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/hiking/feature"],
          "options": {
            "jestConfig": "libs/hiking/feature/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "analytics": false
  },
  "schematics": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "hiking"
}
