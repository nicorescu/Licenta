{
  "version": 1,
  "projects": {
    "hiking-ui": {
      "projectType": "application",
      "root": "apps/hiking-ui",
      "sourceRoot": "apps/hiking-ui/src",
      "prefix": "hkworkspace",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/hiking-ui",
            "index": "apps/hiking-ui/src/index.html",
            "main": "apps/hiking-ui/src/main.ts",
            "polyfills": "apps/hiking-ui/src/polyfills.ts",
            "tsConfig": "apps/hiking-ui/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/hiking-ui/src/favicon.ico",
              "apps/hiking-ui/src/assets"
            ],
            "styles": ["apps/hiking-ui/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/hiking-ui/src/environments/environment.ts",
                  "with": "apps/hiking-ui/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "hiking-ui:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "hiking-ui:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "hiking-ui:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/hiking-ui/src/**/*.ts",
              "apps/hiking-ui/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/hiking-ui/src/test.ts",
            "tsConfig": "apps/hiking-ui/tsconfig.spec.json",
            "karmaConfig": "apps/hiking-ui/karma.conf.js",
            "polyfills": "apps/hiking-ui/src/polyfills.ts",
            "styles": [],
            "scripts": [],
            "assets": []
          }
        }
      }
    },
    "hiking-ui-e2e": {
      "root": "apps/hiking-ui-e2e",
      "sourceRoot": "apps/hiking-ui-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/hiking-ui-e2e/cypress.json",
            "tsConfig": "apps/hiking-ui-e2e/tsconfig.e2e.json",
            "devServerTarget": "hiking-ui:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "hiking-ui:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/hiking-ui-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "shared-app-authenticate": {
      "projectType": "library",
      "root": "libs/shared/app-authenticate",
      "sourceRoot": "libs/shared/app-authenticate/src",
      "prefix": "hkworkspace",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/app-authenticate/src/**/*.ts",
              "libs/shared/app-authenticate/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/app-authenticate"],
          "options": {
            "jestConfig": "libs/shared/app-authenticate/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-app-navigation": {
      "projectType": "library",
      "root": "libs/shared/app-navigation",
      "sourceRoot": "libs/shared/app-navigation/src",
      "prefix": "hkworkspace",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/app-navigation/src/**/*.ts",
              "libs/shared/app-navigation/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/app-navigation"],
          "options": {
            "jestConfig": "libs/shared/app-navigation/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ui-common": {
      "projectType": "library",
      "root": "libs/ui-common",
      "sourceRoot": "libs/ui-common/src",
      "prefix": "hkworkspace",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui-common/src/**/*.ts",
              "libs/ui-common/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui-common"],
          "options": {
            "jestConfig": "libs/ui-common/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "analytics": false
  },
  "schematics": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "hiking-ui"
}
